		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"chattelite": "file:../../sdks/TypeScript/app",
		"chattelite-client": "file:../../sdks/TypeScript/client"
